#. module: e3z_add_delivery_method
#: model:ir.model,name:e3z_add_delivery_method.model_product_product
msgid "Product"
msgstr "Article"

#. module: e3z_add_delivery_method
#: help:res.partner,without_delivery:0
msgid "If checked, delivery carrier won't be add to this customer orders"
msgstr "Si coché, aucune méthode de livraison ne sera ajoutée automatiquement aux commandes passées par ce client"

#. module: e3z_add_delivery_method
#: field:delivery.carrier,min_amount:0
msgid "Minimum amount"
msgstr "Montant minimum"

#. module: e3z_add_delivery_method
#: field:product.product,without_delivery:0
#: field:res.partner,without_delivery:0
msgid "Without delivery"
msgstr "Sans méthode de livraison"

#. module: e3z_add_delivery_method
#: help:product.product,without_delivery:0
msgid "If checked, delivery carrier won't be add to orders with this product"
msgstr "Si coché, aucune méthode de livraison ne sera ajoutée automatiquement aux commandes possédant ce produit"

#. module: e3z_add_delivery_method
#: help:delivery.carrier,default_in_sales:0
msgid "If checked, this delivery method will be added to sale order which have an amount inferior to 1500€"
msgstr "Si coché, cette méthode de livraison sera ajoutée aux commandes de vente ayant un montant inférieur à celui enregistré (1500€ par défaut)"

#. module: e3z_add_delivery_method
#: help:delivery.carrier,min_amount:0
msgid "Under this amount, the delivery carrier will be added to the sale order"
msgstr "Pour les commandes de vente ayant un montant inférieur à cette valeur, cette méthode de livraison sera automatiquement ajoutée auxdites commandes"

